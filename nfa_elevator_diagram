// NFA Elevator Model
digraph {
	start [label="" shape=point]
	start -> F1 [label=start]
	F1 [label=F1 shape=circle]
	F2 [label=F2 shape=circle]
	F3 [label=F3 shape=circle]
	F1 -> F1 [label=req1]
	F1 -> F2 [label=req2]
	F1 -> F3 [label=req2]
	F1 -> F3 [label=req3]
	F2 -> F1 [label=req1]
	F2 -> F2 [label=req2]
	F2 -> F3 [label=req3]
	F3 -> F1 [label=req1]
	F3 -> F2 [label=req2]
	F3 -> F3 [label=req3]
}
